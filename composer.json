{
    "name": "rohsyl/laravel-acl",
    "description": "Access Control List for Laravel",
    "authors": [
        {
            "name": "Sylvain Roh",
            "email": "syzin12@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "illuminate/auth": "^5.8|^6.0",
        "illuminate/container": "^5.8|^6.0",
        "illuminate/contracts": "^5.8|^6.0",
        "illuminate/database": "^5.8|^6.0"
    },
    "autoload": {
        "psr-4": {
            "rohsyl\\LaravelAcl\\": "src/"
        },
        "files": [
            "src/Helpers/acl.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "rohsyl\\LaravelAcl\\": "src/"
        },
        "files": [
            "src/Helpers/acl.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "rohsyl\\LaravelAcl\\ServiceProvider"
            ]
        }
    }
}
